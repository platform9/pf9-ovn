at_fn_diff_devnull "$at_stderr" || at_failed=:
at_fn_diff_devnull "$at_stdout" || at_failed=:
at_fn_check_status 0 $at_status "$at_srcdir/multinode.at:875"
$at_failed && at_fn_log_failure
$at_traceon; }


 { set +x
printf "%s\n" "$at_srcdir/multinode.at:876: podman exec ovn-chassis-1 ip netns exec sw0p1 ip route add default via 10.0.0.1 dev sw0p1"
at_fn_check_prepare_trace "multinode.at:876"
( $at_check_trace; podman exec ovn-chassis-1 ip netns exec sw0p1 ip route add default via 10.0.0.1 dev sw0p1
