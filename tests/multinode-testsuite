  echo Northd waking up
  { set +x
printf "%s\n" "$at_srcdir/ovn-macros.at:979: kill -CONT \$(cat northd/ovn-northd.pid)"
at_fn_check_prepare_notrace 'a $(...) command substitution' "ovn-macros.at:979"
( $at_check_trace; kill -CONT $(cat northd/ovn-northd.pid)
) >>"$at_stdout" 2>>"$at_stderr" 5>&-
at_status=$? at_failed=false
$at_check_filter
at_fn_diff_devnull "$at_stderr" || at_failed=:
